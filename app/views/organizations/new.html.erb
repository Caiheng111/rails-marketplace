


<%= form_with(model: @organization, local: true) do |form| %>
    <div>
        <%= form.label :title %>
        <%= form.text_field :title%>
    </div>
    <div>
       <%= form.label :description %>
        <%= form.text_area :description %>
    </div>
    <div>
       <%= form.label :website %>
        <%= form.text_area :website %>
    </div>
    <div>
       <%= form.label :location %>
        <%= form.text_area :location %>
    </div>
  
    <div>
        <%= form.submit %>
    </div>
<% end %>